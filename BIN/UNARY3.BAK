/*WAP unary- opeartor overloading without friend keyword with return object*/
#include<iostream.h>
#include<conio.h>
class demo
{
 public:
 int a,b;
 demo()
 {
  a=0;
  b=0;
 }
 demo(int a,int b)
 {
 this->a=a;
 this->b=b;
 }
 void display(char *s)
 {
  cout<<s<<"a="<<a<<"\tb="<<b<<endl;
 }
 demo operator-();
};
demo demo::operator-()
{
demo l;
l.a=-a;
l.b=-b;
return l;
}
void main()
{
clrscr();
 demo o(7,4),d;
 cout<<"before unary- operator overloading\n";
 o.display("object o=\n");
 d.display("object d=\n");
 d=-o;     //compiler assume like a d=o.operator-();
 cout<<"After unary- operator overloading\n";
 o.display("object o=\n");
 d.display("object d=\n");
}
