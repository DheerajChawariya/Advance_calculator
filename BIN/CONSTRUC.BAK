#include<iostream.h>
#include<conio.h>
class demo
{
public:
int a,b;
demo()   //default constructor or zero argument constructor
{
 cout<<"enter two number";
 cin>>a>>b;
}
demo(int n)  //one parameterized constructor
{
 a=n;
 b=n;
}
demo(int a,int b) //two parameterized constructor
{
this->a=a;
this->b=b;
}
/*demo(demo &t)   //copy constructor
{
 a=t.a;
 b=t.b;
}*/
void display(char *s)  //normal function
{
cout<<s<<"a="<<a<<"\tb="<<b<<endl;
}
};
void main()
{
 clrscr();
 demo obj;    //implicit calling
 obj.display("object obj=\n");
 demo obj1=demo(); //explicit calling
 obj1.display("object obj1=\n");
 demo o(5);        //implicit calling      demo o=demo(5);
 demo o1=demo(9,6);  //explicit calling    demo o1(9,6);
 o.display("object o=\n");
 o1.display("object o1=\n");
 demo d(o);           //implicit calling
 demo d1=demo(o1);    //explicit calling
 d.display("object d=\n");
 d1.display("object d1=\n");
 demo l=d;
 l.display("object l=\n");
}